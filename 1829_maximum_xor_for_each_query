class Solution {
    public int[] getMaximumXor(int[] nums, int maximumBit) {
        int [] arr=new int [nums.length];
        int max=(int)Math.pow(2,maximumBit)-1;
        int xor=0;
        int l=nums.length-1;
        for(int i=0;i<nums.length;i++)
        {
            xor^=nums[i];
            arr[l]=xor^max;
            l--;
        }       
        return arr;  
    }
}
